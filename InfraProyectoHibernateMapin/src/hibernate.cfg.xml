<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory name="sessionFactori">
        <property name="hibernate.bytecode.use_reflection_optimizer">false</property>
        <property name="hibernate.connection.driver_class">org.postgresql.Driver</property>
        <property name="hibernate.connection.password">adm</property>
        <property name="hibernate.connection.url">jdbc:postgresql:ADMIN</property>
        <property name="hibernate.connection.username">adm</property>
        <property name="hibernate.dialect">org.hibernate.dialect.PostgreSQLDialect</property>
        <mapping class="mx.com.analisispreciosmercado.conf.SeriesOperadas" />
        <mapping class="mx.com.analisispreciosmercado.conf.ConfiguracionSplit" />
        <mapping class="mx.com.analisispreciosmercado.conf.CiConfLinks" />
        <mapping class="mx.com.analisispreciosmercado.conf.SeriesOperadasIntraDia" />
        <mapping class="mx.com.analisispreciosmercado.conf.IpConfTablasHtmlParsear" />
        <mapping class="mx.com.analisispreciosmercado.conf.IpConfPropiedadesTabla" />
        <mapping class="mx.com.analisispreciosmercado.conf.SeriesOperadasBk" />
        <mapping class="mx.com.analisispreciosmercado.conf.Indices" />
        <mapping class="mx.com.analisispreciosmercado.conf.DiasFestivos" />
        <mapping class="mx.com.analisispreciosmercado.conf.IndicesIntraDia" />
    </session-factory>
</hibernate-configuration>
